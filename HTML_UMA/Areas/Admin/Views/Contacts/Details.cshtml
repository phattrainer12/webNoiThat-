@model HTML_UMA.Models.Contact

@{
    ViewBag.Title = "Details";
    Layout = "~/Areas/Admin/Views/Shared/_LayoutAdmin.cshtml";
}

<div class="text-center">
    <h2>Chỉnh sửa</h2>
</div>

<div>
    <h4>Contact</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Tên : @Html.DisplayFor(model => model.contact_LastName)@Html.DisplayFor(model => model.contact_Name)
        </dt>
        <dt>
            Địa chỉ: @Html.DisplayFor(model => model.contact_Address)
        </dt>
        <dt>
            Số điện thoại: @Html.DisplayFor(model => model.contact_Phone)
        </dt>
        <dt>
            Email: @Html.DisplayFor(model => model.contact_Email)
        </dt>
        <dt>
            Nội dung
        </dt>
        <dd>
            @Html.DisplayFor(model => model.contactt_Content)
        </dd>

    </dl>
</div>
<p>
    @Html.ActionLink("Trở về", "Index")
</p>
