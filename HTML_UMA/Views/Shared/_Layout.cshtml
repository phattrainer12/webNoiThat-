<script>
    var color = $('.has-image');
    color.click(function () {
        var thiscl = $(this).data('id-color');
        color.each(function () {
            var cpc = $(this).data('id-color');
            if (thiscl == cpc) {
                $(this).css("border", "1px solid");
                scrolltop();
            } else {
                $(this).css("border", "none");
            }
            
        })
    })
    function scrolltop() {
        $('body, html').animate({ scrollTop: 0 }, 800);
    }
</script>

