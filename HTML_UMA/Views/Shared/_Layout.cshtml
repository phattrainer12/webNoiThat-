                        $j(document.body).on("click", ".btn-cart a", function (event) {
                            event.preventDefault();
                            event.stopPropagation();

                            var $link = $j(event.currentTarget),
                                $button = $link.closest(".button");
                            if ($button.hasClass("stock-alert")) {
                                return false;
                            }
                            //
                            if (!$button.hasClass("disabled") && $link.length) {
                                $button.addClass("disabled").attr("disabled", "disabled");
                                $j.ajax({
                                    url: $link.attr("href"),
                                    method: "POST",
                                    dataType: "json",
                                    data: {
                                        "is_ajax": true
                                    }
                                }).then(
                                    function () {

                                    },
                                    function (xhr, error, obj) {
                                        // console.error("error :: ", error);
                                    }
                                ).always(
                                    function () {
                                        $button.removeClass("disabled").removeAttr("disabled");
                                        $.get(local + '/Product/getCart', function (data) {
                                            $('.cart').find('#mCSB_1_container').html(data);
                                            $('.cart').addClass("active");
                                        });
                                        //auto close after 5 seconds
                                        window.timeout_popup_ids = window.timeout_popup_ids || [];
                                        window.timeout_popup_ids.push(
                                            setTimeout(function () {
                                                $j(".popups-container .popup.cart").removeClass("active");
                                            }, 5000)
                                        );
                                    }
                                );
                            }
                        });
                    });
