                    <script type="text/javascript">
                                /**
                                 * allow to add item to wishlist and show popup dynamically
                                 */
                        var local = window.location.origin;
                                $j(function () {
                                    $j("body").on("click", ".btn-wishlist > a", function (event) {
                                        event.stopPropagation();
                                        event.preventDefault();
                                        var $link = $j(event.currentTarget),
                                            $button = $link.closest(".button");

                                        if (!$button.hasClass("disabled") && $link.length) {
                                            $button.addClass("disabled").attr("disabled", "disabled");
                                            $j.ajax({
                                                url: $link.attr("href"),
                                                method: "POST",
                                                dataType: "json",
                                                data: {
                                                    "is_ajax": true
                                                }
                                            }).then(
                                                function (response) {
                                                    //console.info("success :: ", response);
                                                },
                                                function (xhr, error, obj) {
                                                    //console.error("error :: ", error);
                                                }
                                                ).always(
                                                function () {
                                                    $button.removeClass("disabled").removeAttr("disabled");
                                                    $.get(local+'/Product/getFavourist', function (data) {
                                                        $('.wishlist').find('#mCSB_1_container').html(data);
                                                        $('.wishlist').addClass("active");
                                                        
                                                    }); 
                                                    //MagentoApp.showWishlistPopup(load_wishlist_url);
                                                    // auto close after 5 seconds
                                                    window.timeout_popup_ids = window.timeout_popup_ids || [];
                                                    window.timeout_popup_ids.push(
                                                        setTimeout(function () {
                                                            $j(".popups-container .popup.wishlist").removeClass("active");
                                                        }, 10000)
                                                    );
                                                }
                                                );
                                        }
                                    });
                                });
                    </script>
